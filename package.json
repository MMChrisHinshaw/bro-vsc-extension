{
    "name": "bro",
    "displayName": "bro",
    "description": "Bro Script Language Syntax Highlighting",
    "version": "0.3.0",
    "publisher": "aeppert",
    "engines": {
        "vscode": "^1.12.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/aeppert/bro-vsc-extension.git"
    },
    "categories": [
        "Languages",
        "Formatters"
    ],
    "keywords": [
		"bro",
		"bro-script"
	],
    "contributes": {
        "languages": [
            {
                "id": "bro",
                "aliases": [
                    "Bro",
                    "bro"
                ],
                "extensions": [
                    ".bro"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bro",
                "scopeName": "source.bro",
                "path": "./syntaxes/bro.tmLanguage"
            }
        ]
    }
}
